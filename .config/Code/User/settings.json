{
  "C_Cpp.default.includePath": [
    "${default}"
  ],
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": true
  },
  "editor.formatOnSave": true,
  "editor.scrollBeyondLastLine": false,
  "editor.fontFamily": "'Fira Code', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontSize": 15,
  "editor.fontLigatures": true,
  "editor.snippetSuggestions": "top",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.minimap.enabled": false,
  "editor.trimAutoWhitespace": false,
  "window.zoomLevel": 0,
  "git.enableCommitSigning": true,
  "linesinfostatusbar.totalDisplayFormat": "Lines: %d",
  "linesinfostatusbar.selectedDisplayFormat": " (%d selected)",
  "trailing-spaces.highlightCurrentLine": false,
  "dart.devToolsTheme": "dark",
  "files.eol": "\n",
  "files.autoSave": "afterDelay",
  "workbench.colorTheme": "Material Dark Theme",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "material-icon-theme",
  "go.buildOnSave": "package",
  "go.testOnSave": false,
  "go.coverOnSave": false,
  "go.vetOnSave": "off",
  "go.lintOnSave": "package",
  "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
  "go.alternateTools": {
    "go-langserver": "gopls",
  },
  "go.autocompleteUnimportedPackages": true,
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "[go]": {
    "editor.snippetSuggestions": "bottom",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
  },
  "explorer.confirmDelete": false,
  "C_Cpp.clang_format_fallbackStyle": "Chromium",
  "dart.flutterHotReloadOnSave": false,
  "C_Cpp.default.cppStandard": "c++11",
  "C_Cpp.default.cStandard": "c11",
  "C_Cpp.default.intelliSenseMode": "gcc-x64",
  "markdown.extension.preview.autoShowPreviewToSide": true,
  "yaml.format.enable": true,
  "html.format.enable": false,
  "editor.renderWhitespace": "boundary",
  "vim.useSystemClipboard": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f"
  },
  "vim.debug.silent": true,
  "[yaml]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
  },
  "[ruby]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
  },
  "todo-tree.highlights.defaultHighlight": {
    "icon": "alert",
    "type": "text",
    "foreground": "yellow"
  },
  "todo-tree.tree.showCountsInTree": true,
  "todo-tree.general.statusBar": "tags",
  "python.formatting.provider": "black",
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "editor.formatOnPaste": true,
  "files.autoSaveDelay": 500,
  "python.autoComplete.addBrackets": true,
}