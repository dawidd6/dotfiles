[user]
    email = dawidd0811@gmail.com
    name = Dawid Dziurla
    signingkey = 172E9B0B
    username = dawidd6
[alias]
    lg = "log --graph --pretty=format:'%Cred%h%Creset %Cgreen(%cr)%Creset %C(bold blue)<%an>%Creset %s %C(yellow)%d%Creset'"
    stat = "status -u"
    c = "commit -S -m"
    s = "status"
    d = "diff"
    dc = "diff --cached"
    commie = "commit -S -m"
    del = "restore --staged"
    ls = "ls-tree -r --name-only HEAD"
    puss = "push -u origin"
    amend = "commit -S --amend --no-edit"
    amend-edit = "commit -S --amend --edit"
    cleanout = "!read -p '==> Clean?' var && git clean -fd && git checkout -- ."
    prune-local-branches = "!git branch --merged master | grep -v '^[ *]*master$' | xargs -r git branch -d"
[color]
    ui = auto
[review "review.lineageos.org"]
    username = dawidd6
[commit]
    gpgSign = true
[core]
    excludesfile = ~/.gitignore_global
    pager = diff-so-fancy | less --tabs=4 -RFX
[push]
    default = simple
    followTags = true
[url "git@github.com:dawidd6"]
    insteadOf = "https://github.com/dawidd6"
[color "diff-highlight"]
    oldNormal = red bold
    oldHighlight = red bold 52
    newNormal = green bold
    newHighlight = green bold 22
[color "diff"]
    meta = 11
    frag = magenta bold
    commit = yellow bold
    old = red bold
    new = green bold
    whitespace = red reverse
